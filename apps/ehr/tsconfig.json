{
  "extends": "tsconfig/react.json",
  "compilerOptions": {
    "target": "esnext",
    "lib": ["esnext.array", "dom", "es2022"],
    "module": "esnext",
    "moduleResolution": "node",
    "sourceMap": true,
    "inlineSources": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "baseUrl": ".",
    "paths": {
      "@ehrTheme/*": ["src/themes/ottehr/*"],
      "@ehrDefaultTheme/*": ["src/themes/ottehr/*"]
    }
  },
  "include": [".", ".eslintrc.cjs"]
}
