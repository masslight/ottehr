{
  "extends": "tsconfig/react.json",
  "compilerOptions": {
    "moduleResolution": "node",
    "sourceMap": true,
    "inlineSources": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "baseUrl": ".",
    "paths": {
      "@ehrTheme/*": ["src/themes/ottehr/*"],
      "@ehrDefaultTheme/*": ["src/themes/ottehr/*"]
    }
  },
  "include": [".", ".eslintrc.cjs"]
}
