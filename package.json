{
  "name": "ottehr",
  "version": "1.10.0",
  "private": true,
  "scripts": {
    "lint": "eslint packages/",
    "update": "pnpm recursive up -L -i",
    "build": "sh ./scripts/build.sh",
    "start:local": "pnpm recursive run start:local",
    "start:dev": "pnpm recursive run start:local",
    "start:testing": "pnpm recursive run start:local",
    "start:staging": "pnpm recursive run start:local"
  },
  "packageManager": "pnpm@8.7.6",
  "engines": {
    "pnpm": ">=8.0.0",
    "node": ">=18.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
