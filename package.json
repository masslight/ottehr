{
  "name": "ottehr",
  "version": "1.3.1",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "version": "npm version $npm_package_version --workspaces && git add **/package.json",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "turbo lint",
    "lint:prettier": "prettier --check .",
    "lint:fix": "npm run lint:eslint -- -- --fix & npm run lint:prettier -- --write",
    "intake:start": "turbo start --filter=intake-ui --filter=intake-zambdas",
    "intake:e2e:local": "tsx scripts/run-e2e.ts --app=intake",
    "intake:e2e:local:ui": "tsx scripts/run-e2e.ts --app=intake --ui",
    "intake:e2e:demo": "ENV=demo tsx scripts/run-e2e.ts --app=intake --ui",
    "intake:build": "turbo build --filter=intake-ui --filter=intake-zambdas",
    "intake:lint": "turbo lint --filter=intake-ui... --filter=intake-zambdas...",
    "intake:start:ui": "turbo start --filter=intake-ui",
    "ehr:start": "turbo start --filter=ehr-ui --filter=ehr-zambdas",
    "ehr:e2e:local": "tsx scripts/run-e2e.ts --app=ehr",
    "ehr:e2e:local:ui": "tsx scripts/run-e2e.ts --app=ehr --ui",
    "ehr:e2e:demo": "ENV=demo tsx scripts/run-e2e.ts --app=ehr --ui",
    "ehr:build": "turbo build --filter=ehr-ui --filter=ehr-zambdas",
    "ehr:lint": "turbo lint --filter=ehr-ui... --filter=ehr-zambdas...",
    "ehr:start:ui": "turbo start --filter=ehr-ui",
    "apps:start": "turbo start --filter=intake-ui --filter=intake-zambdas --filter=ehr-ui --filter=ehr-zambdas",
    "test": "turbo test",
    "prepare": "husky"
  },
  "workspaces": [
    "packages/**/*",
    "apps/*"
  ],
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
