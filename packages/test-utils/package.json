{
  "name": "test-utils",
  "type": "module",
  "private": true,
  "version": "1.15.6",
  "main": "lib/main.ts",
  "types": "lib/main.ts",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "lint": "NODE_OPTIONS='--max-old-space-size=8192' eslint --ext .ts,.tsx,.js,.jsx --report-unused-disable-directives --max-warnings 0 .",
