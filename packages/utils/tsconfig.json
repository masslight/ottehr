{
  "extends": "tsconfig/base.json",
  "compilerOptions": {
    "sourceMap": true,
    "inlineSources": true,
    "jsx": "react-jsx",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "target": "esnext",
    "lib": ["esnext.array", "dom"],
  },
  "include": [".", ".eslintrc.cjs"]
}
